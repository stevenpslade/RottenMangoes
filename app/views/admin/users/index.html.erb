<h1>Admin Page</h1>
<hr/>
<h3>Users</h3>

<%= paginate @users %>
<ul>
<% for user in @users %>
<li><%=  user.firstname %> <%=  user.lastname %></li>
<%= link_to "more info", admin_user_path(user) %>
<%= link_to "impersonate this user", impersonate_admin_user_path(user), method: :post %>
<% end %>
</ul>
<p><%= link_to "create", new_admin_user_path %></p>